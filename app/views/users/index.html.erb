<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.username %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Sign In', login_path %>
<%= link_to 'Sign Up', new_user_path %>
